{
    "name": "torans/dingtalk",
    "description": "dingtalk api",
    "type": "sdk",
    "license": "MIT",
    "keywords": [
        "dingtalk",
        "dingding",
        "sdk",
        "钉钉"

    ],
    "authors": [
        {
            "name": "torans",
            "email": "321870617@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "overtrue/http": "^1.1.2",
        "pimple/pimple": "^3.2",
        "psr/simple-cache": "^1.0",
        "monolog/monolog": "2.0.0 as 1.23.0",
        "symfony/cache": "^3.3 || ^4.0",
        "symfony/http-foundation": "^3.2 || ^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EasyDingTalk\\DingtalkServiceProvider"
            ]
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "EasyDingTalk\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EasyDingTalk\\Tests\\": "tests/"
        }
    }
}
